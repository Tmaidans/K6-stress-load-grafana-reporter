{
  "dashboard": {
    "id": null,
    "title": "K6 Load Test - Detailed Analysis Dashboard",
    "tags": ["k6", "load-testing", "detailed"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate Over Time",
        "type": "timeseries",
        "targets": [
          {
            "query": "SELECT derivative(mean(\"value\"), 1s) FROM \"http_reqs\" WHERE $timeFilter GROUP BY time(30s) fill(0)",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Response Time Over Time",
        "type": "timeseries",
        "targets": [
          {
            "query": "SELECT mean(\"value\") FROM \"http_req_duration\" WHERE $timeFilter GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Response Time by Endpoint",
        "type": "timeseries",
        "targets": [
          {
            "query": "SELECT mean(\"value\") FROM \"http_req_duration\" WHERE $timeFilter AND \"endpoint\" = 'Device Information' GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "A"
          },
          {
            "query": "SELECT mean(\"value\") FROM \"http_req_duration\" WHERE $timeFilter AND \"endpoint\" = 'Apps' GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "B"
          },
          {
            "query": "SELECT mean(\"value\") FROM \"http_req_duration\" WHERE $timeFilter AND \"endpoint\" = 'Visibility Views Count' GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "C"
          },
          {
            "query": "SELECT mean(\"value\") FROM \"http_req_duration\" WHERE $timeFilter AND \"endpoint\" = 'Universal Search Recents' GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "D"
          },
          {
            "query": "SELECT mean(\"value\") FROM \"http_req_duration\" WHERE $timeFilter AND \"endpoint\" = 'Threat Activity Graph Tiles' GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Response Time Percentiles",
        "type": "timeseries",
        "targets": [
          {
            "query": "SELECT percentile(\"value\", 50) FROM \"http_req_duration\" WHERE $timeFilter GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "A"
          },
          {
            "query": "SELECT percentile(\"value\", 90) FROM \"http_req_duration\" WHERE $timeFilter GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "B"
          },
          {
            "query": "SELECT percentile(\"value\", 95) FROM \"http_req_duration\" WHERE $timeFilter GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "C"
          },
          {
            "query": "SELECT percentile(\"value\", 99) FROM \"http_req_duration\" WHERE $timeFilter GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Error Rate Over Time",
        "type": "timeseries",
        "targets": [
          {
            "query": "SELECT mean(\"value\") FROM \"http_req_failed\" WHERE $timeFilter GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "Virtual Users Over Time",
        "type": "timeseries",
        "targets": [
          {
            "query": "SELECT mean(\"value\") FROM \"vus\" WHERE $timeFilter GROUP BY time(30s)",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Response Time Distribution",
        "type": "histogram",
        "targets": [
          {
            "query": "SELECT \"value\" FROM \"http_req_duration\" WHERE $timeFilter",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 8,
        "title": "Endpoint Performance Summary",
        "type": "table",
        "targets": [
          {
            "query": "SELECT \"endpoint\", count(\"value\") as \"requests\", mean(\"value\") as \"avg_ms\", percentile(\"value\", 95) as \"p95_ms\", percentile(\"value\", 99) as \"p99_ms\" FROM \"http_req_duration\" WHERE $timeFilter GROUP BY \"endpoint\" ORDER BY \"avg_ms\" DESC",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      },
      {
        "id": 9,
        "title": "Response Time Heatmap",
        "type": "heatmap",
        "targets": [
          {
            "query": "SELECT \"value\" FROM \"http_req_duration\" WHERE $timeFilter",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "ms"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40}
      },
      {
        "id": 10,
        "title": "Request Success Rate",
        "type": "stat",
        "targets": [
          {
            "query": "SELECT 1 - mean(\"value\") FROM \"http_req_failed\" WHERE $timeFilter",
            "rawQuery": true,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0.95},
                {"color": "yellow", "value": 0.99},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}
